{
    "name": "onion/annotations",
    "description": "A minimal annotation package for aspect-based decorators complementing onion/framework",
    "keywords": ["annotations", "phOnion", "onion"],
    "authors": [
        {
            "name": "Dimitar Dimitrov",
            "email": "daghostman.dimitrov@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "roave/security-advisories": "dev-master",
        "doctrine/annotations": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "onion/framework": "2.0",
        "doctrine/cache": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Onion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
